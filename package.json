{
  "name": "@beorn/logger",
  "version": "0.2.0",
  "description": "Structured logging with colors/JSON output",
  "type": "module",
  "module": "src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./worker": {
      "types": "./dist/worker.d.ts",
      "default": "./src/worker.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "picocolors": "^1.1.1"
  }
}
